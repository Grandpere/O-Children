{
  "extends": ["tslint:recommended", "tslint-config-prettier", "tslint-react"],
  "rules": {
    "quotemark": [true, "single", "jsx-double"],
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "object-literal-sort-keys": false,
    "interface-name": false,
    "curly": false
  }
}
